{
    "name": "custom-incognito",
    "version": "1.0",
    "description": "An extension for selective incognito",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "history",
        "contextMenus",
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "options_page": "options.html"
}